    "react-aria-modal": "4.0.0",
    "react-device-detect": "1.6.2",
    "react-dom": "^16.8.6",
    "react-easy-swipe": "0.0.17",
    "react-flickity-component": "^3.2.0",
    "react-ga": "2.5.7",
    "react-helmet": "5.2.0",
    "react-redux": "6.0.1",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scroll-parallax": "^2.0.5",
    "react-select": "2.4.1",
    "redux": "4.0.1",
