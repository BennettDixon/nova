{
  "name": "react-starter",
  "version": "1.0.0",
  "author": "Ryan Warner (https://github.com/RyanWarner)",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "deploy:production": "npm run build:production",
    "deploy:staging": "npm run build:staging && node scripts/deploy.js",
    "build:production": "cross-env NODE_ENV=production DEPLOY_ENV=production node scripts/build.js",
    "build:staging": "cross-env NODE_ENV=production node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "plop": "plop",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "npm run test --updateSnapshot",
    "lint:fix": "eslint src/** --fix"
  },
  "moduleRoots": [
    "src",
    "node_modules"
  ],
  "dependencies": {
    "@babel/runtime": "7.3.4",
    "@tippy.js/react": "2.1.1",
